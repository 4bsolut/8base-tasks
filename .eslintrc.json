{
  "parser": "babel-eslint",
  "plugins": [
    
  ],
  "extends": [
    
    "react-app"
  ],
  "rules": {
    
    "no-console": ["warn"],
    "no-trailing-spaces": "warn",
    "quote-props": ["error", "as-needed"]
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src"]
      }
    }
  }
}
